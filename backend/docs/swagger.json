    "paths": {
        "/auth/signup-token": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Adds a new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "email": "ghanshyam.digiprima@gmail.com"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/signup": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Add a new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "password",
                                    "full_name",
                                    "email",
                                    "role",
                                    "user_photo"
                                ],
                                "properties": {
                                    "password": {
                                        "type": "string"
                                    },
                                    "full_name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "role": {
                                        "type": "string"
                                    },
                                    "user_photo": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                     "full_name":"Naveen Patel",
                                     "email":"np.221196.np@gmail.com",
                                     "password":"123456",
                                     "role":"",
                                     "user_photo":""
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Login user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                   "email":"ghanshyam.digiprima@gmail.com",
                                   "password":"123456"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
          "/auth/change-password": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Change Password",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "oldPassword",
                                    "newPassword"
                                ],
                                "properties": {
                                    "oldPassword": {
                                        "type": "string"
                                    },
                                    "newPassword": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                   "oldPassword":"123456",
                                   "newPassword":"1234567"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/update-user-profile": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Update user profile",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "full_name",
                                    "job_id",
                                    "organization"
                                ],
                                "properties": {
                                    "full_name": {
                                        "type": "string"
                                    },
                                    "organization": {
                                        "type": "string"
                                    },
                                    "job_id": {
                                        "type": "string"
                                    },
                                    "oldPassword": {
                                        "type": "string"
                                    },
                                    "newPassword": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "full_name": "ghanshyam",
                                    "organization": "test",
                                    "job_id": "5e68df7243d3c262c0ab6fd5",
                                    "oldPassword": "",
                                    "newPassword": ""
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/update-user-photo": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Update user photo",
                "description": " `user_photo` field is required.",
                "consumes": "multipart/form-data",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "user_photo": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/forgot-password": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "forgot password",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "email": "ghanshyam.digiprima@gmail.com"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/get-all-users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get all users",
                "requestBody": {
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
