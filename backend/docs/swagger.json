                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "resetToken",
                                    "newPassword",
                                    "confirmPassword"
                                ],
                                "properties": {
                                    "resetToken": {
                                        "type": "string"
                                    },
                                    "newPassword": {
                                        "type": "string"
                                    },
                                    "confirmPassword": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "resetToken": "123456",
                                    "newPassword": "123456",
                                    "confirmPassword": "123456"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/auth/get-all-users": {
